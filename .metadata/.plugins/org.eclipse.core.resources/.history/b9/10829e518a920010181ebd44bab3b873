<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
           xmlns:tempuri="http://tempuri.org/"
           targetNamespace="http://schemas.xmlsoap.org/soap/envelope/"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">

  <!-- Define SOAP Envelope -->
  <xs:element name="Envelope" type="soap:EnvelopeType">
    <xs:annotation>
      <xs:documentation>Root SOAP envelope in the SOAP namespace</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="EnvelopeType">
    <xs:sequence>
      <xs:element name="Body" type="soap:BodyType" minOccurs="1"/>
    </xs:sequence>
    <xs:anyAttribute namespace="##other"/>
  </xs:complexType>

  <xs:complexType name="BodyType">
    <xs:sequence>
      <xs:element ref="tempuri:AddResponse"/>
      <xs:any namespace="##other" minOccurs="0"/>
    </xs:sequence>
    <xs:anyAttribute namespace="##other"/>
  </xs:complexType>

  <!-- Define AddResponse in the tempuri.org namespace -->
  <xs:element name="AddResponse" type="tempuri:AddResponseType">
    <xs:annotation>
      <xs:documentation>AddResponse in the tempuri.org namespace</xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:complexType name="AddResponseType">
    <xs:sequence>
      <xs:element name="AddResult" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>