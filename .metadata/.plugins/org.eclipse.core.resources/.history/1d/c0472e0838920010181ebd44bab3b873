package tests;

import org.testng.annotations.Test;
import static io.restassured.RestAssured.given;

public class ApiKeyTest {

    @Test
    public void testWithoutApiKey() {
        given()
        .when()
            .get("https://reqres.in/api/users?page=2")
        .then()
            .statusCode(200);
    }
}
